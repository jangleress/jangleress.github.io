---
lang: en
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">   
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'home' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'ocean' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'story' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'page_op' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'page' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  
  {% for post in site.posts %}
  {% if post.lang == page.lang and post.layout != nil and post.layout == 'layers-item' %}
  <url>
    <loc>{{site.siteurl}}{{post.url}}</loc>
  </url>
  {% endif %}
  {% endfor %}
  
  
  
</urlset>